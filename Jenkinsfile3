pipeline{
    agent {
        docker {{ image 'node:20.10.0-alpine3.19' }}
    }
    stages{
        stage('check current directory'){
            steps{
                sh 'pwd'
                sh 'whoami'
                sh 'mkdir ansible'
                sh 'python3 --version'
                sh 'npm --version'
            }
        }
    }
}